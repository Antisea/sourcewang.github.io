connect '/C=1 /BAUD=115200'

:START
wait "Hit any key to stop autoboot:"
;mpause 500
send #5
;:AA
;wait "Socle-CDK#"

yesnobox "Change System Clock?" "CAUTION"  ;[message] -[title]
if result=0 goto TEST

sendln "scuclk"	;write "scuclk" to termal
wait "scuclk>"
send "2"
wait "Set CPUCLK:HCLK clock ratio"
wait "scuclk>"
send "2"
wait "confirm the set(y or n)"
send "y"
wait "save to env(y or n)"
send "y"
wait "Socle-CDK"
mpause 500

:TEST
;messagebox "Write command to IP" "- MSG -"
sendln "mw 1d140004 0xff"	; reset SPDIF
mpause 500
sendln "mw 1d140000 0xaa"	; reset SPDIF
mpause 500
sendln "mw 1d140000 0x55"	; reset SPDIF
mpause 500
sendln "mw 1d140000 0xaa"	; reset SPDIF
mpause 500
sendln "mw 1d140000 0x55"		; reset SPDIF
mpause 500
sendln "mw 1d140000 0xaa"	; config threshold
mpause 500
sendln "mw 1d140004 0x55"	; config threshold
mpause 500
sendln "mw 1d140000 0xaa"	; config threshold
mpause 500
sendln "mw 1d140004 0x55"	; config threshold
mpause 500

pause 1 ;delay 0.5s

;messagebox "Start transfer" "- MSG -"

;sendln "mw 1d140004 0xff "	;start transmiter



;enablekeyb 1

goto TEST

